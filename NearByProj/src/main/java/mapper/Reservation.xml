<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.reservation">
	<insert id="insertReservation" parameterType="Reservation"
      useGeneratedKeys="true" keyProperty="number">
    <![CDATA[
       insert into Reservation (pidnum,resdate,restime,name,phone,address,comment,subject,status,doccomment) values 
       (#{pidnum},#{resdate},#{restime},#{name},#{phone},#{address},#{comment},#{subject},#{status},#{doccomment}) 
    ]]>
   </insert>
   
  



</mapper>